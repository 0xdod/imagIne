<div class="card mb-4">
	<div class="card-header">
		<h3 class="mb-0">
			Most Popular
		</h3>
	</div>
	<div class="card-body">
		{{#each sidebar.popular}}
		<div
			class="col-md-4 text-center"
			style="padding-bottom: .5em;">
			<a href="/images/{{uniqueID}}">
				<img
					src="/public/upload/{{filename}}"
					style="width: 75px; height: 75px;"
					class="img-thumbnail"
			/></a>
		</div>
		{{/each}}
	</div>
</div>
